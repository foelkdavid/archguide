#
# ~/.bashrc
#
#force_color_prompt=yes
# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='ls --color=auto'
#PS1='[\u@\h \W]\$ '
PS1='\[\033[1;36m\]\u\[\033[1;31m\]@\[\033[1;32m\]\h:\[\033[1;35m\]\w\[\033[1;31m\]\$\[\033[0m\] '

 export VISUAL=code;
 export EDITOR=code;

 export PATH="/home/$USER/Scripts:$PATH"